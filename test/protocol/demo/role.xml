<?xml version="1.0" encoding="UTF-8"?>
<protocol>
    <struct name="role_info">
        <username type="str" note="用户名"/>
        <name type="string" note="真实姓名"/>
        <mobile type="varchar" note="手机号"/>
        <email type="varchar" note="电子邮件"/>
        <level type="int" note="等级"/>
        <address type="varchar" note="地址"/>
        <gender type="int" note="性别"/>
    </struct>
    <action name="demo_role">
        <request>
            <role_id type="int"/>
        </request>
        <request method="post">
            <role_id type="str"/>
        </request>
        <response>{role_info}</response>
        <response>[role_info]</response>
        <response>[int => role_info]</response>
    </action>
</protocol>