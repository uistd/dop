<?xml version="1.0" encoding="UTF-8"?>
<protocol>
    <model name="blog_list" note="随享基本信息输出">
        <string name="author" note="文章作者"/>
        <bigint name="blogId" note="随享ID"/>
        <string name="cityId" note="城市ID"/>
        <int name="collectNum" note="收藏总数"/>
        <int name="commentNum" note="评论总数"/>
        <list name="contentTagList" note="内容标签集合">
            <string/>
        </list>
        <int name="coverHeight" note="封面图高"/>
        <string name="coverUrl" note="封面图url"/>
        <int name="coverWidth" note="封面图宽"/>
        <bigint name="createTime" note="发布时间"/>
        <int name="downgrade" note="降权值"/>
        <float name="latitude" note="维度"/>
        <int name="likedNum" note="点赞总数"/>
        <float name="longitude" note="经度"/>
        <int name="mediaType" note="随享资源类型。1,全部是图片；2,包含视频"/>
        <string name="originChannel" note="文章来源渠道"/>
        <string name="position" note="位置字符串"/>
        <string name="preview" note="内容预览（默认正文内容的前50个字）"/>
        <string name="provinceId" note="省份ID"/>
        <int name="readNum" note="阅读总数"/>
        <string name="rejectReason" note="最后一次驳回原因"/>
        <int name="sharedNum" note="分享总数"/>
        <int name="status" note="状态:0-未审核;1-已发布;2-后台下线;3-用户删除"/>
        <list name="tagList" note="分类标签集合">
            <string/>
        </list>
        <string name="throwType" note="投放渠道"/>
        <string name="title" note="随享标题"/>
        <int name="type" note="随享类型;1:PGC 2:UGC"/>
        <bigint name="updateTime" note="修改时间"/>
        <string name="userId" note="用户ID"/>
        <string name="weixinDesc" note="微信文案"/>
    </model>

    <model name="blog_pics" note="随享图片视频参数">
        <int name="height" note="图片或视频像素高"/>
        <int name="mediaType" note="图片或视频资源类型-1.图片;2.视频"/>
        <list name="pins" note="图片标记集合">
            <model extend="blog_pins"/>
        </list>
        <bigint name="size" note="视频大小，默认为0"/>
        <string name="url" note="图片或视频url"/>
        <int name="width" note="图片或视频资源像素宽"/>
    </model>

    <model name="blog_recommends" note="随享推荐相关参数">
        <string name="category1" note="一级biz类目"/>
        <string name="category2" note="二级biz类目"/>
        <string name="cityId" note="推荐的城市ID"/>
        <string name="reason" note="推荐理由"/>
        <string name="recId" note="推荐的ID"/>
        <string name="recType" note="推荐类型"/>
    </model>

    <model name="box_pics" note="随享图片输出">
        <bigint name="blogId" note="随享ID"/>
        <int name="height" note="图片像素高"/>
        <int name="mediaType" note="资源类型-1.图片;2.视频"/>
        <bigint name="picId" note="随享图片ID"/>
        <list name="pins" note="图片标记集合">
            <model extend="box_pins"/>
        </list>
        <bigint name="size" note="视频大小"/>
        <string name="url" note="图片url"/>
        <int name="width" note="图片像素宽"/>
    </model>

    <model name="box_recommends" note="随享推荐输出">
        <string name="category1" note="一级biz类目"/>
        <string name="category2" note="二级biz类目"/>
        <string name="cityId" note="推荐的城市ID"/>
        <string name="reason" note="推荐理由"/>
        <string name="recId" note="推荐的ID"/>
        <string name="recType" note="推荐类型"/>
    </model>

    <model name="comment_list" note="评论信息输出">
        <bigint name="blogId" note="随享ID"/>
        <bigint name="commentId" note="评论ID"/>
        <string name="content" note="评论内容"/>
        <bigint name="createTime" note="评论时间"/>
        <bigint name="firstReplyId" note="首条回复的ID"/>
        <int name="likedNum" note="点赞总数"/>
        <int name="replyNum" note="回复总数"/>
        <int name="status" note="状态（1-有效，0-无效）"/>
        <string name="userId" note="发表评论的用户ID"/>
    </model>

    <model name="reply_list" note="回复信息输出">
        <bigint name="blogId" note="随享ID"/>
        <string name="blogUserId" note="随享的用户ID"/>
        <bigint name="commentId" note="评论ID"/>
        <string name="commentUserId" note="评论的用户ID"/>
        <string name="content" note="回复内容"/>
        <bigint name="createTime" note="回复时间"/>
        <int name="likedNum" note="点赞总数"/>
        <bigint name="replyId" note="回复ID"/>
        <int name="status" note="状态（1-有效，0-无效）"/>
        <bigint name="toReplyId" note="回复的随享回复ID，如果是回复评论则填0"/>
        <string name="toReplyUserId" note="回复的随享回复用户ID"/>
        <string name="userId" note="发表回复的用户ID"/>
    </model>

    <model name="topic_list" note="话题数据输出">
        <int name="coverHeight" note="封面图高"/>
        <string name="coverUrl" note="封面图url"/>
        <int name="coverWidth" note="封面图宽"/>
        <bigint name="createTime" note="创建时间戳"/>
        <string name="description" note="话题描述"/>
        <int name="recommendStatus" note="推荐状态"/>
        <int name="status" note="状态"/>
        <list name="tagList" note="标签集合">
            <string/>
        </list>
        <string name="title" note="话题标题"/>
        <bigint name="topicId" note="话题ID"/>
        <int name="type" note="话题类型（1: 频道话题、2:日常话题）"/>
        <bigint name="updateTime" note="更新时间戳"/>
        <string name="userId" note="用户ID"/>
    </model>

    <model name="blog_pins" note="随享图片标记参数">
        <int name="locHeight" note="图片打标垂直位置(千分整型)"/>
        <int name="locWidth" note="图片打标水平位置(千分整型)"/>
        <string name="name" note="标记名"/>
        <string name="type" note="标记类型"/>
        <string name="value" note="标记值"/>
    </model>

    <model name="box_pins" note="随享图片标记输出">
        <bigint name="blogId" note="随享ID"/>
        <int name="locHeight" note="图片打标垂直位置(千分整型)"/>
        <int name="locWidth" note="图片打标水平位置(千分整型)"/>
        <string name="name" note="标记名"/>
        <bigint name="picId" note="随享图片ID"/>
        <bigint name="pinId" note="随享图片标记ID"/>
        <string name="type" note="标记类型"/>
        <string name="value" note="标记值"/>
    </model>

    <action name="get_blog" note="基本信息列表查询">
        <request method="get">
            <string name="blogIds" note="批量随享ID,多个ID英文逗号分隔"/>
        </request>
        <response extend="/api/result">
            <model name="data" class_name="get_blog_data">
                <list name="list">
                    <model extend="blog_list"/>
                </list>
                <int name="pageNum" note="当前页数"/>
                <int name="pageSize" note="每页记录数"/>
                <int name="pages" note="总页数"/>
                <int name="size" note="当前页记录数"/>
                <int name="total" note="总数"/>
            </model>
        </response>
    </action>

    <action name="post_blog" note="创建随享">
        <request method="post">
            <string name="author" note="文章作者"/>
            <string name="cityId" note="城市ID"/>
            <int name="collectNum" note="收藏总数"/>
            <string name="content" note="随享内容"/>
            <list name="contentTagList" note="内容标签集合">
                <string/>
            </list>
            <string name="contentType" note="随享内容类型 - 1,文本；2,H5"/>
            <int name="coverHeight" note="封面图高"/>
            <string name="coverUrl" note="封面图url"/>
            <int name="coverWidth" note="封面图宽"/>
            <float name="latitude" note="纬度"/>
            <int name="likedNum" note="点赞总数"/>
            <float name="longitude" note="经度"/>
            <string name="originChannel" note="文章来源渠道"/>
            <list name="pics" note="随享图片集合">
                <model extend="blog_pics"/>
            </list>
            <string name="position" note="位置字符串"/>
            <string name="preview" note="内容预览"/>
            <string name="provinceId" note="省份ID"/>
            <int name="readNum" note="阅读总数"/>
            <list name="recommends" note="随享推荐集合">
                <model extend="blog_recommends"/>
            </list>
            <list name="tagList" note="分类标签集合">
                <string/>
            </list>
            <string name="throwType" note="投放渠道,多渠道用英文逗号分隔.默认为1(首页-精选),可能取值:1-精选(包含电影美食逛街等tab页);2-发现(行动派)"/>
            <string name="title" note="随享标题"/>
            <int name="type" note="随享类型，1:PGC 2:UGC 3:KOL"/>
            <string name="userId" note="用户ID"/>
            <string name="weixinDesc" note="微信文案"/>
        </request>
        <response extend="/api/result">
            <bigint name="data"/>
        </response>
    </action>

    <action name="get_blogId" note="基本信息详情">
        <request method="get">
            <bigint name="blogId" note="随享ID"/>
        </request>
        <response extend="/api/result">
            <model name="data" class_name="get_blogId_data">
                <string name="author" note="文章作者"/>
                <bigint name="blogId" note="随享ID"/>
                <string name="cityId" note="城市ID"/>
                <int name="collectNum" note="收藏总数"/>
                <int name="commentNum" note="评论总数"/>
                <list name="contentTagList" note="内容标签集合">
                    <string/>
                </list>
                <int name="coverHeight" note="封面图高"/>
                <string name="coverUrl" note="封面图url"/>
                <int name="coverWidth" note="封面图宽"/>
                <bigint name="createTime" note="发布时间"/>
                <int name="downgrade" note="降权值"/>
                <float name="latitude" note="维度"/>
                <int name="likedNum" note="点赞总数"/>
                <float name="longitude" note="经度"/>
                <int name="mediaType" note="随享资源类型。1,全部是图片；2,包含视频"/>
                <string name="originChannel" note="文章来源渠道"/>
                <string name="position" note="位置字符串"/>
                <string name="preview" note="内容预览（默认正文内容的前50个字）"/>
                <string name="provinceId" note="省份ID"/>
                <int name="readNum" note="阅读总数"/>
                <string name="rejectReason" note="最后一次驳回原因"/>
                <int name="sharedNum" note="分享总数"/>
                <int name="status" note="状态:0-未审核;1-已发布;2-后台下线;3-用户删除"/>
                <list name="tagList" note="分类标签集合">
                    <string/>
                </list>
                <string name="throwType" note="投放渠道"/>
                <string name="title" note="随享标题"/>
                <int name="type" note="随享类型;1:PGC 2:UGC"/>
                <bigint name="updateTime" note="修改时间"/>
                <string name="userId" note="用户ID"/>
                <string name="weixinDesc" note="微信文案"/>
            </model>
        </response>
    </action>

    <action name="put_blogId" note="修改随享">
        <request method="put">
            <bigint name="blogId" note="随享ID"/>
            <string name="author" note="文章作者"/>
            <string name="cityId" note="城市ID"/>
            <int name="collectNum" note="收藏总数"/>
            <string name="content" note="随享内容"/>
            <list name="contentTagList" note="内容标签集合">
