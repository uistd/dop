            </list>
            <string name="contentType" note="随享内容类型 - 1,文本；2,H5"/>
            <int name="coverHeight" note="封面图高"/>
            <string name="coverUrl" note="封面图url"/>
            <int name="coverWidth" note="封面图宽"/>
            <float name="latitude" note="纬度"/>
            <int name="likedNum" note="点赞总数"/>
            <float name="longitude" note="经度"/>
            <string name="originChannel" note="文章来源渠道"/>
            <list name="pics" note="随享图片集合">
                <model extend="blog_pics"/>
            </list>
            <string name="position" note="位置字符串"/>
            <string name="preview" note="内容预览"/>
            <string name="provinceId" note="省份ID"/>
            <int name="readNum" note="阅读总数"/>
            <list name="recommends" note="随享推荐集合">
                <model extend="blog_recommends"/>
            </list>
            <list name="tagList" note="分类标签集合">
                <string/>
            </list>
            <string name="throwType" note="投放渠道,多渠道用英文逗号分隔.默认为1(首页-精选),可能取值:1-精选(包含电影美食逛街等tab页);2-发现(行动派)"/>
            <string name="title" note="随享标题"/>
            <int name="type" note="随享类型，1:PGC 2:UGC 3:KOL"/>
            <string name="userId" note="用户ID"/>
            <string name="weixinDesc" note="微信文案"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="delete_blogId" note="删除">
        <request method="delete">
            <bigint name="blogId" note="随享ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="get_box" note="图文信息详情">
        <request method="get">
            <bigint name="blogId" note="随享ID"/>
        </request>
        <response extend="/api/result">
            <model name="data" class_name="get_box_data">
                <string name="author" note="文章作者"/>
                <bigint name="blogId" note="随享ID"/>
                <string name="cityId" note="城市ID"/>
                <int name="collectNum" note="收藏总数"/>
                <int name="commentNum" note="评论总数"/>
                <string name="content" note="随享内容"/>
                <list name="contentTagList" note="内容标签集合">
                    <string/>
                </list>
                <int name="contentType" note="内容类型. 1,文本；2,H5"/>
                <int name="coverHeight" note="封面图高"/>
                <string name="coverUrl" note="封面图url"/>
                <int name="coverWidth" note="封面图宽"/>
                <bigint name="createTime" note="发布时间"/>
                <int name="downgrade" note="降权值"/>
                <float name="latitude" note="维度"/>
                <int name="likedNum" note="点赞总数"/>
                <float name="longitude" note="经度"/>
                <int name="mediaType" note="随享资源类型。1,全部是图片；2,包含视频"/>
                <string name="originChannel" note="文章来源渠道"/>
                <list name="pics" note="随享图片集合">
                    <model extend="box_pics"/>
                </list>
                <string name="position" note="位置字符串"/>
                <string name="preview" note="内容预览（默认正文内容的前50个字）"/>
                <string name="provinceId" note="省份ID"/>
                <int name="readNum" note="阅读总数"/>
                <list name="recommends" note="随享推荐集合">
                    <model extend="box_recommends"/>
                </list>
                <string name="rejectReason" note="最后一次驳回原因"/>
                <int name="sharedNum" note="分享总数"/>
                <int name="status" note="状态:0-未审核;1-已发布;2-后台下线;3-用户删除"/>
                <list name="tagList" note="分类标签集合">
                    <string/>
                </list>
                <string name="throwType" note="投放渠道"/>
                <string name="title" note="随享标题"/>
                <int name="type" note="随享类型;1:PGC 2:UGC"/>
                <bigint name="updateTime" note="修改时间"/>
                <string name="userId" note="用户ID"/>
                <string name="weixinDesc" note="微信文案"/>
            </model>
        </response>
    </action>

    <action name="post_collect" note="新增收藏">
        <request method="post">
            <bigint name="blogId" note="随享ID"/>
            <string name="userId" note="收藏的用户ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="delete_collect" note="删除">
        <request method="delete">
            <bigint name="blogId" note="随享ID"/>
            <string name="userId" note="收藏的用户ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="get_comment" note="列表">
        <request method="get">
            <bigint name="blogId" note="随享ID"/>
            <string name="commentIds" note="评论ID,多个ID英文逗号分隔"/>
            <int name="status" note="状态"/>
            <int name="page" note="当前页码,从1开始"/>
            <int name="size" note="每页记录数"/>
        </request>
        <response extend="/api/result">
            <model name="data" class_name="get_comment_data">
                <list name="list">
                    <model extend="comment_list"/>
                </list>
                <int name="pageNum" note="当前页数"/>
                <int name="pageSize" note="每页记录数"/>
                <int name="pages" note="总页数"/>
                <int name="size" note="当前页记录数"/>
                <int name="total" note="总数"/>
            </model>
        </response>
    </action>

    <action name="post_comment" note="发表评论">
        <request method="post">
            <bigint name="blogId" note="随享ID"/>
            <string name="content" note="评论内容"/>
            <string name="userId" note="发表评论的用户ID"/>
        </request>
        <response extend="/api/result">
            <model name="data" class_name="post_comment_data">
                <bigint name="blogId" note="随享ID"/>
                <bigint name="commentId" note="评论ID"/>
                <string name="content" note="评论内容"/>
                <bigint name="createTime" note="评论时间"/>
                <bigint name="firstReplyId" note="首条回复的ID"/>
                <int name="likedNum" note="点赞总数"/>
                <int name="replyNum" note="回复总数"/>
                <int name="status" note="状态（1-有效，0-无效）"/>
                <string name="userId" note="发表评论的用户ID"/>
            </model>
        </response>
    </action>

    <action name="get_commentId" note="评论详情">
        <request method="get">
            <bigint name="blogId" note="随享ID"/>
            <bigint name="commentId" note="评论ID"/>
        </request>
        <response extend="/api/result">
            <model name="data" class_name="get_commentId_data">
                <bigint name="blogId" note="随享ID"/>
                <bigint name="commentId" note="评论ID"/>
                <string name="content" note="评论内容"/>
                <bigint name="createTime" note="评论时间"/>
                <bigint name="firstReplyId" note="首条回复的ID"/>
                <int name="likedNum" note="点赞总数"/>
                <int name="replyNum" note="回复总数"/>
                <int name="status" note="状态（1-有效，0-无效）"/>
                <string name="userId" note="发表评论的用户ID"/>
            </model>
        </response>
    </action>

    <action name="delete_commentId" note="删除">
        <request method="delete">
            <bigint name="blogId" note="随享ID"/>
            <bigint name="commentId" note="评论ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="post_like" note="赞评论">
        <request method="post">
            <bigint name="blogId" note="所属随想ID"/>
            <bigint name="commentId" note="评论ID"/>
            <string name="userId" note="点赞用户ID，如果用户未登录，可能是设备ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="delete_like" note="取消评论赞">
        <request method="delete">
            <bigint name="blogId" note="所属随想ID"/>
            <bigint name="commentId" note="评论ID"/>
            <string name="userId" note="点赞用户ID，如果用户未登录，可能是设备ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="put_off" note="不通过">
        <request method="put">
            <bigint name="blogId" note="随享ID"/>
            <bigint name="commentId" note="评论ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="put_pass" note="通过">
        <request method="put">
            <bigint name="blogId" note="随享ID"/>
            <bigint name="commentId" note="评论ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="put_downgrade" note="降权">
        <request method="put">
            <bigint name="blogId" note="随享ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="post_like_blogId" note="赞随享">
        <request method="post">
            <bigint name="blogId" note="随想ID"/>
            <string name="userId" note="点赞用户ID，如果用户未登录，可能是设备ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="delete_like_blogId" note="取消随享赞">
        <request method="delete">
            <bigint name="blogId" note="随想ID"/>
            <string name="userId" note="点赞用户ID，如果用户未登录，可能是设备ID"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="put_off_blogId" note="下线">
        <request method="put">
            <bigint name="blogId" note="随享ID"/>
            <string name="reason" note="下线原因"/>
        </request>
        <response extend="/api/result">
        </response>
    </action>

    <action name="put_publish" note="上线">
