<?xml version="1.0" encoding="UTF-8"?>
<protocol>
    <struct name="test_arr">
        <string name="name"/>
        <string name="mobile"/>
        <int name="age"/>
    </struct>
    <data name="test_data">
        <int8 name="int8"/>
        <uint8 name="uint8"/>
        <int16 name="int16"/>
        <uint16 name="uint16"/>
        <int name="int32">
            <mock begin="1" step="2"/>
        </int>
        <uint name="uint"/>
        <int64 name="int64"/>
        <string name="string">
            <mock pair="int32" map="1:2, 3:4, 5:6"/>
        </string>
        <float name="float32"/>
        <double name="double64"/>
        <binary name="binary"/>
        <bool name="is_ok" default="false"/>
        <list name="list"><string/></list>
        <list name="list_list" keep_name="1"><list name="list"><int/></list></list>
        <map name="map"><int/><string/></map>
        <struct name="null_struct">
            <int name="no_value"/>
        </struct>
        <struct name="test_struct" keep_name="true">
            <string name="first_name">
                <mock range="10, 20"/>
            </string>
            <string name="last_name">
                <mock use="test_struct/first_name"/>
            </string>
            <int8 name="gender"/>
        </struct>
        <struct name="test_arr" extend="test_arr"/>
    </data>
    
    <data name="simple_data">
        <int8 name="int8"/>
        <uint8 name="uint8"/>
        <int16 name="int16"/>
        <uint16 name="uint16"/>
        <int name="int32"/>
        <uint name="uint"/>
        <int64 name="int64"/>
        <string name="string"/>
        <float name="float32"/>
        <double name="double64"/>
        <binary name="binary"/>
        <bool name="is_ok" default="false"/>
        <list name="list"><string/></list>
        <map name="map"><int/><string/></map>
    </data>
</protocol>