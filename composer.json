{
  "name": "ffan/dop",
  "description": "FFan DOP",
  "keywords": [
    "ffan",
    "dop"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ffan/php-utils": "*"
  },
  "autoload": {
    "psr-4": {
      "ffan\\dop\\" : "base",
      "ffan\\dop\\coder\\" : "coder",
      "ffan\\dop\\plugin\\" : "plugin"
    }
  },
  "minimum-stability": "dev",
  "archive": {
    "exclude": [
      "test",
      ".gitignore"
    ]
  },
  "config": {
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/private/"
    },
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/packagist/"
    },
    {
      "packagist": false
    }
  ]
}