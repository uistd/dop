{
  "name": "ffan/dop",
  "description": "FFan DOP",
  "keywords": [
    "ffan",
    "dop"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ffan/std-common": "2.*",
    "ffan/std-console": "1.*",
    "ffan/std-git": "1.*"
  },
  "autoload": {
    "psr-4": {
      "FFan\\Dop\\" : "base",
      "FFan\\Dop\\Coder\\" : "coder",
      "FFan\\Dop\\Plugin\\" : "plugin",
      "FFan\\Dop\\Tool\\" : "tool"
    }
  },
  "minimum-stability": "stable",
  "archive": {
    "exclude": [
      "test",
      ".gitignore"
    ]
  },
  "config": {
    "secure-http": false
  }
}