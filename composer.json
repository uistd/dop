{
  "name": "ffan/dop",
  "description": "FFan DOP",
  "keywords": [
    "ffan",
    "dop"
  ],
  "type": "composer",
  "authors": [
    {
      "name": "huangshunzhao",
      "email": "huangshunzhao@wanda.cn",
      "role": "Founder"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "ffan/std-common": "2.*",
    "ffan/std-console": "1.*"
  },
  "autoload": {
    "psr-4": {
      "FFan\\Dop\\" : "base",
      "FFan\\Dop\\Protocol\\": "base/protocol",
      "FFan\\Dop\\Build\\": "base/build",
      "FFan\\Dop\\Coder\\" : "coder",
      "FFan\\Dop\\Plugin\\" : "plugin"
    }
  },
  "minimum-stability": "dev",
  "archive": {
    "exclude": [
      "test",
      ".gitignore"
    ]
  },
  "config": {
    "secure-http": false
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/private/"
    },
    {
      "type": "composer",
      "url": "http://composer.intra.ffan.com/repo/packagist/"
    },
    {
      "packagist": false
    }
  ]
}